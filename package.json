{
  "name": "text-blind-watermark-js",
  "version": "1.0.0",
  "description": "A JavaScript/TypeScript implementation of text blind watermarking using zero-width Unicode characters",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "watermark",
    "text-watermark",
    "blind-watermark",
    "invisible-watermark",
    "steganography",
    "zero-width-characters",
    "unicode",
    "text-hiding",
    "information-hiding"
  ],
  "author": {
    "name": "qiupo",
    "email": "895665504@qq.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiupo/text-blind-watermark-js.git"
  },
  "bugs": {
    "url": "https://github.com/qiupo/text-blind-watermark-js/issues"
  },
  "homepage": "https://github.com/qiupo/text-blind-watermark-js#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "rollup": "^3.29.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
